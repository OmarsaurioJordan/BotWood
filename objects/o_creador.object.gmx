                    audio_play_sound(a_edi_poner, 99, false);
                }
                break;
            
            case 17:
                if !instance_exists(o_paz)
                {
                    instance_create(x, y, o_paz);
                    audio_play_sound(a_edi_poner, 99, false);
                }
                break;
        }
    }
}
