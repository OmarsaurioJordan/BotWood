        <arguments>
          <argument>
            <kind>1</kind>
            <string>if !o_game.pausa
{
    reloj_proy -= dlt;
    if reloj_proy &lt;= 0 or !instance_exists(origen)
    {
        instance_create(x, y, o_exploproy);
        audio_play_sound(a_proy_fin, 5, false);
        instance_destroy();
    }
    else
    {
        //mover y detectar colision
        var otro;
        if reloj_proy &gt;= m_distadisp * 0.9
        {
            otro = s_colision(x, y, m_rad_m * 0.5, o_altobloq, m_rad_m, 1, origen);
        }
        else
        {
            otro = s_colision(x, y, m_rad_m * 0.5, o_altobloq, m_rad_m, 1, -1);
        }
        if otro != noone
        {
            var sis = true;
            
            switch otro.object_index
            {
                case o_edificio:
                    if otro.estructura == 2 { s_demoledificio(otro, false); }
                    else { s_demoledificio(otro, true); }
                    audio_play_sound(a_proy_impact, 20, false);
                    break;
                
                case o_robot:
                    if o_game.inmune and origen.object_index == o_robot
                    {
                        otro.origen.calor = m_calor;
                    }
                    else if o_game.onepunch
                    {
                        s_destruido(otro);
                    }
                    else
                    {
                        s_calculadamage(otro, angle_difference(otro.direction,
                            point_direction(otro.x, otro.y, x, y)), false);
                    }
                    repeat 5
                    {
                        s_particulas(otro.x + 16 - irandom(32), otro.y + 16 - irandom(32), true, 0);
                    }
                    audio_play_sound(a_proy_impact, 20, false);
                    break;
                
                case o_globo:
                    otro.impulso = m_velglobo;
                    otro.direction = point_direction(x, y, otro.x, otro.y);
                    audio_play_sound(a_proy_impact, 20, false);
                    audio_play_sound(a_map_aire, 5, false);
                    break;
                
                case o_pavo:
                case o_roca:
                    direction = point_direction(otro.x, otro.y, x, y);
                    x += lengthdir_x(m_veldisp * dlt, direction);
                    y += lengthdir_y(m_veldisp * dlt, direction);
                    sis = false;
                    audio_play_sound(a_proy_rebot, 20, false);
                    break;
                
                case o_cortina:
                    audio_play_sound(a_proy_impact, 20, false);
                    break;
                
                case o_arbol:
                    s_arbolcosas(otro, false);
                    audio_play_sound(a_proy_arbol, 20, false);
                    break;
                
                case o_torre:
                    with otro
                    {
                        instance_create(x, y, o_explobot);
                        var h = instance_create(x, y, o_decorado);
                        h.direction = 9;
                        instance_destroy();
                    }
                    audio_play_sound(a_proy_impact, 20, false);
                    break;
                
                case o_asediador:
                    with otro
                    {
                        instance_create(x, y, o_explobot);
                        var h = instance_create(x, y, o_decorado);
                        if retirada
                        {
                            h.direction = 36;
                        }
                        else
                        {
                            h.direction = 10;
                        }
                        instance_destroy();
                    }
                    audio_play_sound(a_proy_impact, 20, false);
                    break;
            }
            
            instance_create(x, y, o_exploproy);
            if sis
            {
                instance_destroy();
            }
        }
        else
        {
            x += lengthdir_x(m_veldisp * dlt, direction);
            y += lengthdir_y(m_veldisp * dlt, direction);
            depth = -y;
            // movimiento perseguidor
            if origen.object_index == o_robot {
                var lis = ds_list_create();
                var pts;
                pts[0, 0] = x + lengthdir_x(110, direction + 18);
                pts[0, 1] = y + lengthdir_y(110, direction + 18);
                pts[1, 0] = x + lengthdir_x(110, direction - 18);
                pts[1, 1] = y + lengthdir_y(110, direction - 18);
                var yo = id;
                if o_game.tipomapa &gt;= 3 {
                    with o_robot {
                        if id != yo.origen {
                            if point_in_triangle(x, y, yo.x, yo.y,
                                    pts[0, 0], pts[0, 1], pts[1, 0], pts[1, 1]) and
                                    !collision_line(x, y, yo.x, yo.y, o_bloque, true, false) {
                                ds_list_add(lis, id);
                            }
                        }
                    }
                }
                if o_game.tipomapa == 2 {
                    with o_globo {
                        if id != yo.origen {
                            if point_in_triangle(x, y, yo.x, yo.y,
                                    pts[0, 0], pts[0, 1], pts[1, 0], pts[1, 1]) and
                                    !collision_line(x, y, yo.x, yo.y, o_bloque, true, false) {
                                ds_list_add(lis, id);
                            }
                        }
                    }
                }
                if o_game.tipomapa == 0 {
                    with o_asediador {
                        if id != yo.origen {
                            if point_in_triangle(x, y, yo.x, yo.y,
                                    pts[0, 0], pts[0, 1], pts[1, 0], pts[1, 1]) and
                                    !collision_line(x, y, yo.x, yo.y, o_bloque, true, false) {
                                ds_list_add(lis, id);
                            }
                        }
                    }
                }
                var nn = ds_list_size(lis);
                if nn &gt; 0 {
                    var xx = 0;
                    var yy = 0;
                    var aux;
                    for (var i = 0; i &lt; nn; i++) {
                        aux = ds_list_find_value(lis, i);
                        xx += aux.x;
                        yy += aux.y;
                    }
                    xx /= nn;
                    yy /= nn;
                    var pd = point_direction(x, y, xx, yy);
                    var dd = angle_difference(direction, pd);
                    direction -= min(abs(dd), 0.2 * dlt) * sign(dd);
                }
                ds_list_destroy(lis);
            }
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
